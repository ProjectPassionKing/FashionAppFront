<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.fashionapp.View.FirstFragment"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_MainFragment"
            app:destination="@+id/main_fragment" />

    </fragment>
    <fragment
        android:id="@+id/main_fragment"
        android:name="com.example.fashionapp.View.MainFragment"
        android:label=""
        tools:layout="@layout/fragment_main" />

    <fragment
        android:id="@+id/diagnosis_fragment"
        android:name="com.example.fashionapp.View.DiagnosisFragment"
        android:label=""
        tools:layout="@layout/fragment_diagnosis" />

    <fragment
        android:id="@+id/ResultFragment"
        android:name="com.example.fashionapp.View.ResultFragment"
        android:label=""
        tools:layout="@layout/fragment_result" />

    <fragment
        android:id="@+id/CordiTipFragment"
        android:name="com.example.fashionapp.View.CordiTipFragment"
        android:label=""
        tools:layout="@layout/fragment_cordi_tip" />
    <fragment
        android:id="@+id/RecommandFragment"
        android:name="com.example.fashionapp.View.RecommandFragment"
        android:label=""
        tools:layout="@layout/fragment_recommand" />
    <fragment
        android:id="@+id/TakePhotoFragment"
        android:name="com.example.fashionapp.View.TakePhotoFragment"
        android:label=""
        tools:layout="@layout/fragment_take_photo" />

    <fragment
        android:id="@+id/TakeSimFragment"
        android:name="com.example.fashionapp.View.TakeSimFragment"
        android:label=""
        tools:layout="@layout/fragment_take_sim" />
    <fragment
        android:id="@+id/AllinOneFragment"
        android:name="com.example.fashionapp.View.AllInOneFragment"
        android:label=""
        tools:layout="@layout/fragment_all_in_one" />

    <fragment
        android:id="@+id/ShowPhotoFragment"
        android:name="com.example.fashionapp.View.ShowPhotoFragment"
        android:label=""
        tools:layout="@layout/fragment_show_photo" />
    <fragment
        android:id="@+id/ShowSimFragment"
        android:name="com.example.fashionapp.View.ShowSimFragment"
        android:label=""
        tools:layout="@layout/fragment_show_sim" />

    <fragment
        android:id="@+id/DiagnosisSelectFragment"
        android:name="com.example.fashionapp.View.DiagnosisSelectFragment"
        android:label=""
        tools:layout="@layout/fragment_diagnosis_select" />

    <fragment
        android:id="@+id/DiagnosisPicFragment"
        android:name="com.example.fashionapp.View.DiagnosisPicFragment"
        android:label=""
        tools:layout="@layout/fragment_diagnosis_pic" />

    <action
        android:id="@+id/action_global_AllInOneFragment"
        app:destination="@+id/AllinOneFragment"/>
    <action
        android:id="@+id/action_global_DiagnosisSelectFragment"
        app:destination="@id/DiagnosisSelectFragment" />
    <action
        android:id="@+id/action_global_TakePhotoFragment"
        app:destination="@id/TakePhotoFragment" />
    <action
        android:id="@+id/action_global_CordiTipFragment"
        app:destination="@id/CordiTipFragment" />

    <action
        android:id="@+id/action_global_TakeSimFragment"
        app:destination="@id/TakeSimFragment" />

    <action
        android:id="@+id/action_global_toHome"
        app:destination="@+id/main_fragment" />
    <action
        app:destination="@id/RecommandFragment"
        android:id="@+id/action_global_RecommandFragment"/>

    <action
        app:destination="@+id/diagnosis_fragment"
        android:id="@+id/action_global_diagnosisFragment" />

    <action
        app:destination="@+id/ResultFragment"
        android:id="@+id/action_global_ResultFragment" />

    <action
        app:destination="@+id/DiagnosisPicFragment"
        android:id="@+id/action_global_DiagnosisPicFragment" />

    <action
        app:destination="@+id/ShowPhotoFragment"
        android:id="@+id/action_global_ShowPhotoFragment" />

    <action
        app:destination="@+id/ShowSimFragment"
        android:id="@+id/action_global_ShowSimFragment" />

    <action
        app:destination="@+id/FirstFragment"
        android:id="@+id/action_global_FirstFragment" />
</navigation>